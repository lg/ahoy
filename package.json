{
  "name": "ahoy",
  "displayName": "Ahoy",
  "description": "A VSCode extension to browse a Kubernetes cluster",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ahoy.helloWorld",
        "title": "Hello World"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "ahoyExplorer",
          "name": "Ahoy"
        }
      ]
    },
    "statusbar": []
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . --watch"
  },
  "dependencies": {
    "@types/vscode": "^1.83.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.15.0",
    "typescript": "^5.2.2"
  }
}
